{
	"name": "hello-sveltekit2",
	"version": "0.0.1",
	"scripts": {
		"start": "node build",
		"dev": "vite dev",
		"dev:3000": "PORT=3000 vite dev",
		"dev:4000": "PORT=4000 vite dev",
		"dev:5000": "PORT=5000 vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"drizzle:generate": "drizzle-kit generate",
		"drizzle:migrate": "drizzle-kit migrate",
		"drizzle:studio": "drizzle-kit studio",
		"drizzle:seed": "tsx ./src/lib/server/db/seed.ts",
		"drizzle:custom": "drizzle-kit generate --custom"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.2.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
		"@tailwindcss/forms": "^0.5.8",
		"@types/eslint": "^8.56.7",
		"@types/lodash": "^4.17.7",
		"@types/postmark": "^1.4.1",
		"@types/socket.io": "^3.0.1",
		"autoprefixer": "^10.4.19",
		"bits-ui": "^0.22.0",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"lucide-svelte": "^0.474.0",
		"paneforge": "^0.0.6",
		"postcss": "^8.4.38",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^5.0.4",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.4.1",
		"tsx": "^4.11.2",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0-alpha.20",
		"vite": "^5.3.2",
		"vitest": "^1.2.0"
	},
	"type": "module",
	"dependencies": {
		"@types/bcrypt": "^5.0.2",
		"axios": "^1.7.8",
		"bcrypt": "^5.1.1",
		"clsx": "^2.1.1",
		"dotenv": "^16.4.5",
		"drizzle-kit": "^0.26.2",
		"drizzle-orm": "^0.35.2",
		"drizzle-zod": "^0.5.1",
		"jsonwebtoken": "^9.0.2",
		"lodash": "^4.17.21",
		"pg": "^8.13.1",
		"postgres": "^3.4.4",
		"postmark": "^4.0.5",
		"socket.io": "^4.8.1",
		"tailwind-merge": "^2.3.0",
		"tailwind-variants": "^0.2.1",
		"tailwindcss-animated": "^1.1.2",
		"uuid": "^11.1.0",
		"zod": "^3.23.8"
	}
}
